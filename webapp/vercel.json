{
  "version": 2,
  "name": "telegram-bot-webapp",
  "buildCommand": "bash build-env.sh",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
